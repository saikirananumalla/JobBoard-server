type Query {
    company(id: ID!): Company
    job(id: ID!): Job
    jobs: [Job!]
}

type Company {
    id: ID!
    name: String!
    description: String
    jobs: [Job!]
}

"""
Represents a job type posted on our site. Documentation comment.
"""
#regular comment
type Job {
    id: ID!
    """ The Date of Job Creation ex. 2022-12-31"""
    date: String!
    title: String
    company: Company!
    description: String
}
